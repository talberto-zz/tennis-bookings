# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)

# Resource booking
GET		/bookings					@controllers.BookingsController.index
POST	/bookings					@controllers.BookingsController.create
GET		/bookings/newForm			@controllers.BookingsController.newForm
POST	/bookings/:id/delete		@controllers.BookingsController.delete(id: Long)
GET		/bookings/:id				@controllers.BookingsController.show(id: Long)
